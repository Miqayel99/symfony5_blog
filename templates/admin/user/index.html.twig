{% extends 'admin/adminbase.html.twig' %}
{% block body %}
    <div class="container-fluid">
    <div class="row no-gutters">
        <div class="col-12">
            <h1 class="display-4 text-center mt-5">
                {{ title }}
            </h1>
        </div>
    </div>
    <div class="container-fluid ">
    <div class="row no-gutters mt-2">
        <div class="col-12">
            <a href="{{ path('admin_user_create') }}" class="btn btn-success">Create new user</a><hr>

        </div>
    </div>

    <div class="container-fluid">
    <div class="row no-gutters">
        <div class="col-12">
            <h2>Users list</h2>
            <ul>
                {% for user in users %}
                    <li>{{ user.email }}  |  {{ user.roles | json_encode }}</li>
                {% endfor %}
            </ul>
        </div>
    </div>



{% endblock %}